import java.util.Scanner;
public class Pattern6
{
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int row=5;
        for(int i=row;i>=i;i--)
        {
            for(int j=row;j>=i;j--)
            {
                System.out.print(" ");
            }
           for(int k=1;k<=i;k++)
           {
               System.out.print("*");
           }
           System.out.println();
        }
    }
}
