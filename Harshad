import java.util.Scanner;
public class Harshard
{
    public static void main(String[] args)
    {
       Scanner sc=new Scanner(System.in);
       int a=sc.nextInt();
       int b=0,t;
       int m=a;
       while(a>0)
       {
           t=a%10;
           b+=t;
           a/=10;
       }
     if(m%b==0)
        System.out.println("Harshad Number");
     else
     System.out.println("Not Harshad Number");
        
}
}



