import java.util.*;
public class String4{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        char c;
        String s1="";
        for(int i=s.length()-1;i>=0;i--)
        {
          c=s.charAt(i);
          s1=s1+c;
        }
        System.out.println(s1);
    }
}
