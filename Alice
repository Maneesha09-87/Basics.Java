import java.util.Scanner;
public class Alice
{
    public static void main(String args[])
    {
      Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      int sum=0;
      int t;
      t=n%10;
      sum=sum+t;
      n=n/10;
      t=n%10;
      sum=sum+t;
      n=n/10;
      System.out.println(sum);
    }
}
